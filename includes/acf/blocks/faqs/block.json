{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "acf/faqs",
  "title": "FAQs",
  "description": "A custom FAQs block that uses ACF fields.",
  "category": "widgets",
  "icon": "list-view",
  "keywords": ["faq", "question", "answer"],
  "acf": {
    "mode": "auto",
    "renderTemplate": "faqs.php"
  },
  "supports": {
    "anchor": true,
    "align": false,
    "align_text": false,
    "multiple": true,
    "spacing": {
      "margin": [ "top", "bottom"]
    }
  }
}